APP_NAME=agent-builder-hub
APP_VERSION=1.0.0
APP_ENV=development
API_PREFIX=/api/v1
MAX_WORKERS=4
DEBUG=true
LOG_LEVEL=debug
SECRET_KEY=your-secret-key

# AWS Configuration
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=AKIAXXXXXXXXXXXXXXXX
AWS_SECRET_ACCESS_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
AWS_SESSION_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Database Configuration
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/dbname
DB_MAX_CONNECTIONS=20
DB_MIN_CONNECTIONS=5
DB_TIMEOUT=30

# Authentication
COGNITO_USER_POOL_ID=us-west-2_xxxxxx
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
JWT_SECRET_KEY=your-jwt-secret-key
JWT_ALGORITHM=HS256

# External Services
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
MAVENLINK_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
LEVER_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Security
CORS_ORIGINS=http://localhost:3000,https://*.hakkoda.io
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600