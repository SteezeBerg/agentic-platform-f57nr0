    "@aws-amplify/ui-react": "^6.0.0",
    "@aws-amplify/ui-react-i18n": "^6.0.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "4.6.0",
    "@react-hook/media-query": "^1.1.1",
