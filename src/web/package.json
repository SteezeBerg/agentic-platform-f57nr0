    "husky": "^8.0.3",
    "jest": "^29.0.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.0",
